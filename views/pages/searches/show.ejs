<body class="result">
    <header>
        <h1>Welcome</h1>
    </header>
    <nav>
        Google Search
    </nav>
    <div class="parent">
            <% arrayBook.map(valueData =>{%>
                <div class="child">
                <img src="<%= valueData.image;%>" alt="">
                <h2 class="title"><%= valueData.title;%> </h2>
                <h2><%= valueData.author;%> </h2>
                <p><%= valueData.description;%> </p>
            </div>
                <% }); %>
    </div >

</body>

</html>